{
    "expo": {
        "name": "KidBux",
        "slug": "kidbux",
        "platforms": ["ios", "android", "web"],
        "version": "0.1",
        "orientation": "portrait",
        "icon": "./src/assets/icon.png",
        "splash": {
            "image": "./src/assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "bundleIdentifier": "com.birkoss.kidbux",
            "buildNumber": "1",
            "supportsTablet": false
        },
        "android": {
            "package": "com.birkoss.kidbux",
            "versionCode": 1
        }
    }
}
