{
    "expo": {
        "name": "Walleteur",
        "slug": "walleteur",
        "platforms": ["ios", "android", "web"],
        "version": "0.1",
        "orientation": "portrait",
        "icon": "./src/assets/icon.png",
        "splash": {
            "image": "./src/assets/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#FF4C58"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "bundleIdentifier": "com.birkoss.walleteur",
            "buildNumber": "1",
            "supportsTablet": false,
            "config": {
                "usesNonExemptEncryption": false
            }
        },
        "android": {
            "package": "com.birkoss.walleteur",
            "versionCode": 2,
            "permissions": []
        }
    }
}
